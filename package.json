{
  "name": "react-components-1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://Istiniel.github.io/RSSchool-React/",
  "scripts": {
    "dev": "ts-node server.ts",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node server",
    "build": "npm run lint && tsc --noEmit && vite build --base=./",
    "lint": "eslint . --ext .tsx --ext .ts",
    "lint-fix": "eslint . --ext .tsx --ext .ts --fix",
    "test:unit": "npx vitest run --coverage.enabled --coverage.provider=c8 --coverage.all",
    "cypress": "cypress run --e2e",
    "cypress:open": "cypress open --e2e --browser chrome",
    "cy": "npx rimraf coverage && npx rimraf .nyc_output && start-server-and-test dev http://localhost:5173 cypress && npx nyc report && npx nyc report --reporter=text-summary",
    "cy:open": "npx rimraf coverage && npx rimraf .nyc_output && start-server-and-test dev http://localhost:5173 cypress:open && npx nyc report && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@cypress/request": "^2.88.11",
    "@reduxjs/toolkit": "^1.9.3",
    "babel-preset-es2015": "^6.24.1",
    "compression": "^1.7.4",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/webpack-preprocessor": "^5.17.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jest-localstorage-mock": "^2.4.26",
    "jsdom": "^21.1.1",
    "lint-staged": "^12.4.1",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^5.0.0",
    "sass": "^1.59.3",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-node": "^0.30.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  }
}
